pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build')
        {
            steps
            {
                echo 'hi there'
            }
        }
        stage('deploy')
        {
            steps
            {
                echo 'hi hello'
            }
        }
    }
}
